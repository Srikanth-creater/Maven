<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
	<servlet>
		<servlet-name>a</servlet-name>
		<servlet-class>com.demo.AddServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>a</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>b</servlet-name>
		<servlet-class>com.demo.Squareserv</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>b</servlet-name>
		<url-pattern>/sq</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>abc</servlet-name>
		<servlet-class>com.demo.Servconfigandcontext</servlet-class>
		
		<init-param>
			<param-name>name</param-name>
			<param-value>Sriramulu Savithri</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>abc</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<context-param>
		<param-name>name</param-name>
		<param-value>Srikanth</param-value>
	</context-param>
	<context-param>
		<param-name>mobile</param-name>
		<param-value>Xiomi</param-value>
	</context-param>	
</web-app>